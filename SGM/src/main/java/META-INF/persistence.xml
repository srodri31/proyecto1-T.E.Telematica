<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
    http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
	version="1.0">
	<persistence-unit name="SGMPU" transaction-type="JTA">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
		<!-- To JbossServer use java:/datasources/SGMDS or java:jboss/datasources/SGMDS -->
		<jta-data-source>java:jboss/datasources/telematica</jta-data-source>
		<class>com.dis.eafit.sgm.modelo.Multa</class>



	</persistence-unit>
</persistence>
