<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
  
            <navigation-rule>
            <from-view-id>/XHTML/multa.xhtml</from-view-id>
            <navigation-case>
                <from-outcome>goInitialMenu</from-outcome>
                <to-view-id>/XHTML/initialMenu.xhtml</to-view-id>
                <redirect />
            </navigation-case>
        </navigation-rule>
        <navigation-rule>
            <from-view-id>/XHTML/multaListDataTable.xhtml</from-view-id>
            <navigation-case>
                <from-outcome>goInitialMenu</from-outcome>
                <to-view-id>/XHTML/initialMenu.xhtml</to-view-id>
                <redirect />
            </navigation-case>
        </navigation-rule>
        <navigation-rule>
            <from-view-id>/XHTML/multaListDataTableEditable.xhtml</from-view-id>
            <navigation-case>
                <from-outcome>goInitialMenu</from-outcome>
                <to-view-id>/XHTML/initialMenu.xhtml</to-view-id>
                <redirect />
            </navigation-case>
        </navigation-rule>
        <navigation-rule>
            <from-view-id>/XHTML/initialMenu.xhtml</from-view-id>
            <navigation-case>
                <from-outcome>goMulta</from-outcome>
                <to-view-id>/XHTML/multa.xhtml</to-view-id>
                <redirect />
            </navigation-case>
        </navigation-rule>
        <navigation-rule>
            <from-view-id>/XHTML/initialMenu.xhtml</from-view-id>
            <navigation-case>
                <from-outcome>goMultaListDataTable</from-outcome>
                <to-view-id>/XHTML/multaListDataTable.xhtml</to-view-id>
                <redirect />
            </navigation-case>
        </navigation-rule>
        <navigation-rule>
            <from-view-id>/XHTML/initialMenu.xhtml</from-view-id>
            <navigation-case>
                <from-outcome>goMultaListDataTableEditable</from-outcome>
                <to-view-id>/XHTML/multaListDataTableEditable.xhtml</to-view-id>
                <redirect />
            </navigation-case>
        </navigation-rule>
    </faces-config>
